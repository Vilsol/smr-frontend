{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"sourceMap": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"allowJs": true,
		"checkJs": true,
		"paths": {
			"$app/*": [".svelte-kit/runtime/app/*"],
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"]
		}
	},
	"include": [
		"src/**/*.d.ts",
	]
}