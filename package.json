{
  "name": "smr-next",
  "version": "0.0.1",
  "scripts": {
    "build": "run-s translations build:static build:node",
    "build:staging": "cross-env NODE_ENV=staging run-s build:static build:node",
    "build:production": "cross-env NODE_ENV=production run-s build:static build:node",
    "build:node": "rimraf .svelte build/node && cross-env SVELTE_ADAPTER=node vite build",
    "build:static": "rimraf .svelte build/static && vite build",
    "check": "svelte-check --ignore build,.svelte,.svelte-kit",
    "check:dev": "svelte-check --ignore build,.svelte,.svelte-kit --watch",
    "dev": "run-p -r graphql-codegen:watch dev:serve check:dev lint:dev",
    "dev:serve": "vite dev",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "graphql-codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "lint": "eslint . --ext .ts,.svelte && prettier --check --plugin-search-dir=. .",
    "lint:dev": "chokidar -c \"eslint . --ext .ts,.svelte --cache\" --silent --initial \"src/**/*.svelte\" \"src/**/*.ts\"",
    "prepare": "husky install",
    "postinstall": "svelte-kit sync",
    "start": "svelte-kit start",
    "format": "prettier --write --plugin-search-dir=. . && eslint . --fix --ext .ts,.svelte",
    "translations": "sh ./download_translations.sh"
  },
  "dependencies": {
    "@felte/core": "^1.3.7",
    "@felte/reporter-svelte": "=1.1.5",
    "@felte/validator-zod": "^1.0.13",
    "@floating-ui/dom": "^1.5.1",
    "@tolgee/svelte": "^5.9.12",
    "@urql/core": "^4.0.7",
    "@urql/exchange-auth": "^2.1.2",
    "@urql/exchange-graphcache": "^6.0.4",
    "@urql/exchange-persisted": "^4.0.0",
    "@urql/introspection": "^1.0.2",
    "@urql/svelte": "^4.0.1",
    "custom-protocol-check": "^1.4.0",
    "felte": "^1.2.7",
    "graphql": "^16.6.0",
    "js-cookie": "^3.0.5",
    "jsdom": "^22.0.0",
    "jszip": "^3.10.1",
    "marked": "^5.1.0",
    "marked-gfm-heading-id": "^3.0.4",
    "marked-mangle": "^1.1.0",
    "prismjs": "^1.29.0",
    "semver": "^7.5.0",
    "socket.io-client": "^4.6.1",
    "tslib": "^2.5.0",
    "wonka": "^6.3.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cfworker/json-schema": "^1.12.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typed-document-node": "^4.0.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-document-nodes": "^3.0.4",
    "@graphql-codegen/typescript-graphql-files-modules": "^2.2.1",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@graphql-codegen/urql-introspection": "^2.2.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@rollup/plugin-terser": "^0.4.1",
    "@skeletonlabs/skeleton": "^2.0.0",
    "@skeletonlabs/tw-plugin": "^0.1.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.24.1",
    "@types/js-cookie": "^3.0.3",
    "@types/marked": "^4.3.0",
    "@types/node": "^20.1.1",
    "@types/prismjs": "^1.26.0",
    "@types/semver": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "autoprefixer": "^10.4.14",
    "chokidar-cli": "^3.0.0",
    "cookieconsent": "^3.1.1",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "dompurify": "^3.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-import-url": "^7.2.0",
    "postcss-load-config": "^4.0.1",
    "postcss-preset-env": "^8.3.2",
    "prettier": "~2.8.8",
    "prettier-plugin-svelte": "~2.9.0",
    "rimraf": "^5.0.0",
    "schema-dts": "^1.1.2",
    "svelte": "^3.59.1",
    "svelte-adapter-bun": "^0.5.0",
    "svelte-check": "^3.5.1",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "urql": "^4.0.2",
    "vite": "^4.3.5",
    "vite-plugin-tailwind-purgecss": "^0.1.3"
  },
  "type": "module",
  "engines": {
    "node": ">= 18.0.0"
  }
}